# Application Settings
APP_NAME=UAVcast-Free
APP_VERSION=1.0.0
DEBUG=false

# Server
HOST=0.0.0.0
PORT=8000
RELOAD=false

# Database
DATABASE_URL=sqlite+aiosqlite:///./uavcast.db

# CORS Origins (comma-separated)
CORS_ORIGINS=*

# MAVLink Defaults
DEFAULT_SERIAL_PORT=/dev/ttyACM0
DEFAULT_BAUD_RATE=57600
DEFAULT_TELEMETRY_PORT=14550

# Video Defaults
DEFAULT_VIDEO_PORT=5600
DEFAULT_VIDEO_RESOLUTION=1280x720
DEFAULT_VIDEO_FPS=30
DEFAULT_VIDEO_BITRATE=2000
